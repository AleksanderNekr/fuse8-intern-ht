{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information"
    }
  },
  "AllowedHosts": "*",
  "Currencies": {
    "DefaultCurrency": "RUB",
    "DecimalPlace": 2
  },
  "GrpcAddress": "http://localhost:5666",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "System": "Error",
        "Microsoft": "Error"
      }
    },
    "Properties": {
      "Site": "Website"
    },
    "WriteTo:Console": {
      "Name": "Console",
      "Args": {
        "outputTemplate": "[SERILOG]{Timestamp:dd.MM.yy HH:mm:ss.fff} [{Level:u3}] {Message:lj} Properties: {Properties}{NewLine}{Exception}"
      }
    }
  },
  "ConnectionStrings": {
    "LocalConnection": "Host=localhost;Port=5432;Database=postgres;Username=postgres;Password=123qwe"
  }
}
